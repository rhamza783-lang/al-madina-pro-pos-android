<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
      Base application theme. This is a very simple theme to prevent resource conflicts.
      The parent 'NoActionBar' is important.
    -->
    <style name="Theme.AlMadinaPOS" parent="android:Theme.Material.Light" />

    <!--
      The specific theme applied to the Activity in the Manifest.
      This removes the top title bar from the old Android View system.
    -->
    <style name="Theme.AlMadinaPOS.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>
